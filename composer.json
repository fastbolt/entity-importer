{
    "name": "fastbolt/entity-importer",
    "type": "library",
    "description": "A library for importing CSV files to doctrine entities",
    "keywords": [
        "fastbolt",
        "csv",
        "import",
        "entity-import",
        "doctrine"
    ],
    "homepage": "https://github.com/fastbolt/entity-importer",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Hirtzbruch",
            "email": "dhh@fastbolt.com",
            "homepage": "https://github.com/dhirtzbruch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Fastbolt\\EntityImporter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fastbolt\\EntityImporter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "prefer-stable": true
}
